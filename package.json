  "scripts": {
    "build": "nest build",
    "build:prod": "nest build -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts --runInBand",
    "test:cov:all": "jest --coverage --testRegex='.*\\.(spec|e2e-spec)\\.ts$' --runInBand --detectOpenHandles --testTimeout=50000",
    "prepare": "node -e \"try { if(process.env.NODE_ENV !== 'production') require('husky').install(); } catch (e) {}\"",
    "commit": "git-cz",
    "sonar": "ts-node sonar-project.ts"
  },
